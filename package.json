{
  "name": "react-native-css-media-query-processor",
  "version": "0.13.1",
  "description": "Match style objects containing CSS Media Queries with React Native",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --ignore src/__tests__,src/__mocks__",
    "precommit": "lint-staged",
    "release": "npmpub",
    "benchmark": "yarn build && node benchmark.js",
    "test": "jest"
  },
  "author": "Krister Kari",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "!**/__tests__",
    "!**/__mocks__",
    "!**/perf-tests",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "React",
    "ReactNative",
    "react-native",
    "styles",
    "CSS",
    "Media Queries"
  ],
  "bugs": {
    "url": "https://github.com/kristerkari/react-native-css-media-query-processor/issues"
  },
  "homepage": "https://github.com/kristerkari/react-native-css-media-query-processor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristerkari/react-native-css-media-query-processor.git"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "deepmerge": "^2.1.0",
    "micro-memoize": "^2.0.1"
  },
  "peerDependencies": {
    "react-native": ">=0.45.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "npmpub": "^3.1.0",
    "prettier": "^1.12.0",
    "rimraf": "^2.6.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.js",
      "!src/mediaquery.js"
    ],
    "coverageDirectory": "./.coverage/",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
