{
  "name": "react-native-css-media-query-processor",
  "version": "0.10.0",
  "description": "Match style objects containing CSS Media Queries with React Native",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --ignore src/__tests__,src/__mocks__",
    "precommit": "lint-staged",
    "release": "npmpub",
    "test": "jest"
  },
  "author": "Krister Kari",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "!**/__tests__",
    "!**/__mocks__",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "React",
    "ReactNative",
    "react-native",
    "styles",
    "CSS",
    "Media Queries"
  ],
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "css-mediaquery": "^0.1.2"
  },
  "peerDependencies": {
    "react-native": ">=0.45.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-preset-es2015": "^6.24.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "npmpub": "^3.1.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none"
  }
}
